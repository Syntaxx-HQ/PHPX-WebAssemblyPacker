#!/usr/bin/env php
<?php

foreach ([__DIR__ . '/../../../autoload.php', __DIR__ . '/../vendor/autoload.php'] as $file) {
    if (file_exists($file)) {
        require $file;
        break;
    }
}

foreach ([__DIR__ . '/../../../filepackager.php', __DIR__ . '/../filepackager.php'] as $file) {
    if (file_exists($file)) {
        require $file;
        break;
    }
}
